{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "UI",
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "jsx": "react-jsx",
    "lib": ["ES2015", "DOM"],
    "module": "esnext"
  }
}
